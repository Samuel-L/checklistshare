{
  "name": "checklistshare",
  "version": "1.0.0",
  "description": "A web application where you can create password-protected checklists and share them.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:prod": "npm test -- -u",
    "jest:watch": "jest --watch",
    "jest:coverage": "jest --coverage",
    "webpack": "webpack-dev-server --config ./webpack.config.dev.js",
    "start": "npm run webpack",
    "clean-dist": "npm run remove-dist && mkdir ./client/dist",
    "remove-dist": "rimraf ./client/dist",
    "build:html": "babel-node ./tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test:prod build:html",
    "build": "babel-node ./tools/build.js",
    "postinstall": "npm run build"
  },
